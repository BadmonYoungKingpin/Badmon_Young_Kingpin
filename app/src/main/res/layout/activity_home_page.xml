<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".HomePage"
    android:background="#39B54A"
    android:contextClickable="true">

    <ImageView
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/foodNumber"
        android:background="@drawable/foodNo"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="362dp"/>

    <ImageView
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/workNumber"
        android:background="@drawable/workNo"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="190dp"/>

    <ImageView
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/extraNo"
        android:background="@drawable/extraNo"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="240dp"
        android:padding="0dp"/>

    <ImageView
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/physicalNumber"
        android:background="@drawable/physicalNo"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="60dp"
        android:padding="0dp"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="350dp"
        android:id="@+id/work"
        android:background="@drawable/workButtonHome"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="350dp"
        android:id="@+id/physical"
        android:background="@drawable/physicalButtonHome"
        android:layout_alignTop="@+id/work"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="350dp"
        android:id="@+id/other"
        android:background="@drawable/otherButtonHome"
        android:layout_alignTop="@+id/work"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="350dp"
        android:id="@+id/food"
        android:background="@drawable/foodButtonHome"
        android:layout_alignTop="@+id/work"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="350dp"
        android:id="@+id/outline"
        android:background="@drawable/outsideEnclosure"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/workButtonText"
        android:layout_marginTop="104dp"
        android:text="WORK"
        android:textSize="20dp"
        android:layout_marginRight="51dp"
        android:layout_marginEnd="51dp"
        android:layout_alignTop="@+id/work"
        android:layout_toLeftOf="@+id/physicalButtonText"
        android:layout_toStartOf="@+id/physicalButtonText" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/physicalButtonText"
        android:text="PHYSICAL"
        android:textSize="20dp"
        android:layout_alignTop="@+id/workButtonText"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="51dp"
        android:layout_marginEnd="51dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText"
        android:layout_marginTop="67dp"
        android:text="FOOD"
        android:textSize="20dp"
        android:layout_below="@+id/workButtonText"
        android:layout_alignRight="@+id/workButtonText"
        android:layout_alignEnd="@+id/workButtonText" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:layout_alignBottom="@+id/editText"
        android:layout_toRightOf="@+id/workButtonText"
        android:layout_toEndOf="@+id/workButtonText" android:text="OTHER"
        android:textSize="20dp" />




</RelativeLayout>
